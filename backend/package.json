{
  "name": "dilsaycare-backend",
  "version": "1.0.0",
  "description": "Backend API for DilSayCare Application",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "migrate": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node node_modules/knex/bin/cli.js migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node node_modules/knex/bin/cli.js migrate:rollback --knexfile knexfile.ts",
    "migrate:status": "cross-env NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node node_modules/knex/bin/cli.js migrate:status --knexfile knexfile.ts",
    "migrate:make": "ts-node node_modules/knex/bin/cli.js migrate:make --knexfile knexfile.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
